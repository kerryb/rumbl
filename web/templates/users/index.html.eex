<h2>Users</h2>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Username</th>
      <th>Name</th>
      <th></th>
  </thead>
  <tbody>
    <%= for user <- @users do %>
      <tr>
        <td><%= user.username %></td>
        <td><%= user.name %></td>
        <td><%= link "view", to: users_path(@conn, :show, user.id) %></td>
      </tr>
    <%= end %>
  </tbody>
</table>
